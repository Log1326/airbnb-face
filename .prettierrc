{
  "trailingComma": "none",
  "useTabs": true,
  "tabWidth": 4,
  "semi": false,
  "jsxSingleQuote": true,
  "singleQuote": true,
  "arrowParens": "always",
  "importOrder": [
    "THIRD_PARTY_MODULES",
    "^@/components/(.*)$",
    "^../(.*)",
    "^./(.*)",
    "^(.scss)"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
